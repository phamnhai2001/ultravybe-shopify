.nav_item>li>a.accordion_navi {
    display: inline-block;
    max-width: 100%;
    width: 100%;
    position: relative;
    padding: 17px 0;
    font-weight: 500;
}

.nav_item .content,
.nav_item .content li {
    padding: 0;
    margin: 0;
}

.nav_item .content li a {
    display: inline-block;
    max-width: 100%;
    width: 100%;
    position: relative;
    font-weight: 500;
}
.nav_item.inner_drown_nav .content li a {
    font-size: 16px;
    line-height: 24px;
    text-align: left;
    padding: 17px 0;
}
.nav_item.inner_drown_nav .content.drown_nav_list li {
    padding-left: 15px;
}

.nav_item>li>a.accordion_navi>span:after,
.nav_item .content li a span:after {
    transform: rotate(0deg);
    -webkit-transition: all 0.5s 0s cubic-bezier(0.215, 0.61, 0.355, 1);
    transition: all 0.5s 0s cubic-bezier(0.215, 0.61, 0.355, 1);
    content: '';
    display: block;
    position: absolute;
    top: 50%;
    right: 2.5%;
    width: 27px;
    height: 27px;
    margin-top: -14px;
    background-image: url({{'sidemenu_icon_arrow.png' | asset_url}});
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 100% auto;
}

.nav_item>li>a.accordion_navi.main>span:after {
    -webkit-transition: all 0.5s 0s cubic-bezier(0.215, 0.61, 0.355, 1);
    transition: all 0.5s 0s cubic-bezier(0.215, 0.61, 0.355, 1);
    content: '';
    display: block;
    position: absolute;
    top: 50%;
    right: 2.5%;
    width: 27px;
    height: 27px;
    margin-top: -14px;
    background-image: url({{'sidemenu_icon_arrow.png' | asset_url}});
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 100% auto;
    transform: rotate(90deg);
}

.nav_item>li>a.accordion_navi.main.active>span:after {
    transform: rotate(-90deg);
}


